{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "airbnb-typescript", "prettier"],
    "plugins": ["@typescript-eslint", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "project": ["./tsconfig.json"]
    },
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "react/jsx-filename-extension": "off"
    }
}
